const log = require('logToConsole');
const setInWindow = require('setInWindow');
log('data =', data);
const injectScript = require('injectScript');
const addEventCallback = require('addEventCallback'); //capture all events
const isConsentGranted = require('isConsentGranted');

// Call data.gtmOnSuccess when the tag is finished. 
data.gtmOnSuccess();
const statisticsEnabled = data.statistics;
// Assuming you've defined fields for necessary configuration values
const config = {
    token: data.TokenID, // Use the provided field
    statistics: {
        enabled: data.statistics && isConsentGranted("analytics_storage"),
        cookieLessTracking: data.cookieless && isConsentGranted("analytics_storage"),
        documenttracking: data.documenttracking && isConsentGranted("analytics_storage") ? {
          enabled: true,
          documentCls: data.documentCls,
          documentIgnoreCls: data.documentIgnoreCls,
          documentExt: data.documentExt ? [] : ["pdf","doc","ppt","docx","pptx"]
        } : false
    },
    heatmap: {
        enabled: data.heatmap && isConsentGranted("analytics_storage"),
    },
    pageCorrect: {
        enabled: data.pageCorrect,
    },
    pageAssistV2: {
      enabled: data.pageassistv2 && isConsentGranted("functionality_storage"),
      theme: data.pageassistTheme || "light",
      mainColor: data.pageassistMainColor || "#783CE2",
      textColor: data.pageassistTextColor || "#ffffff",
      linkColor: data.pageassistLinkColor || "#783CE2",
      buttonHoverColor: data.pageassistButtonHoverColor || "#783CE2",
      mainDarkColor: data.pageassistMainDarkColor || "#052942",
      textDarkColor: data.pageassistTextDarkColor || "#ffffff",
      linkColorDark: data.pageassistLinkColorDark || "#FFCF4B",
      buttonHoverDarkColor: data.pageassistButtonHoverDarkColor || "#FFCF4B",
      greeting: data.pageassistGreeting || "Discover your personalization options",
      direction: data.pageassistDirection || "leftbottom",
      coordinates: data.pageassistCoordinates || "",
      iconShape: data.pageassistIconShape || "circle",
      title: data.pageassistTitle || "Personalization Options",
      titleText: data.pageassistTitleText || "Welcome to PageAssist™ toolbar! Adjust the options below to cater the website to your accessibility needs.",
      iconPictureUrl: data.pageassistIconPictureUrl || "logo",
      logoPictureUrl: data.pageassistLogoPictureUrl || "",
      logoPictureBase64: data.pageassistLogoPictureBase64 || "",
      languages: data.pageassistLanguages ? data.pageassistLanguages.split(',') : ["en-US", "da-DK", "nl-NL", "pt-PT", "fr-FR"],
      defaultLanguage: data.pageassistDefaultLanguage || "en-US",
      skipTo: data.pageassistSkipTo || false,
      customInformationText: data.pageassistCustomInformationText || "Personalisation",
      alwaysOnTop: data.pageassistAlwaysOnTop || false
    }
};



setInWindow('_monsido', config, true);

injectScript('https://app-script.monsido.com/v2/monsido-script.js');


